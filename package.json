{
  "name": "@jmvtrinidad/chimp",
  "description": "Your development companion for doing quality, faster.",
  "version": "0.0.0-development",
  "author": "Lukasz Gandecki lukasz@xolv.io",
  "bin": {
    "chimp": "./bin/run"
  },
  "bugs": "https://github.com/xolvio/chimp/issues",
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/merge": "^8.3.6",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "debug": "^4.3.1",
    "enquirer": "^2.3.6",
    "find-package-json": "^1.2.0",
    "graphql": "16.6.0",
    "graphql-tag": "^2.10.3",
    "handlebars": "^4.0.12",
    "listr2": "3.3.3",
    "ora": "^5.3.0",
    "pascal-case": "^3.1.2",
    "shelljs": "^0.8.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/node": "^14.14.22",
    "chai": "^4.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.3.1",
    "globby": "^10",
    "jest": "^26.6.3",
    "nyc": "^14",
    "prettier": "^2.2.1",
    "testdouble": "^3.16.1",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^8",
    "typescript": "4.4.4",
    "wait-on": "^5.2.1"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/scaffold",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/xolvio/chimp",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "chimp",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "release": {
    "branches": ["main"]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --cache",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme && copyfiles -u 1 src/generate/templates/* src/generate/runtime-config-helpers/* lib",
    "test": "jest",
    "version": "oclif-dev readme && git add README.md",
    "type-check": "tsc --noEmit",
    "end-to-end-test": "ts-node ./src/scripts/end-to-end-test.ts"
  },
  "types": "lib/index.d.ts"
}
